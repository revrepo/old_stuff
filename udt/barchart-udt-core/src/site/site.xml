<project name="${project.name}">

  <skin>
  	<groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.3.0</version>
  </skin>

  <custom>
    <fluidoSkin>
      <gitHub>
        <ribbonColor>orange</ribbonColor>
        <ribbonOrientation>right</ribbonOrientation>
        <projectId>barchart/barchart-udt</projectId>
        <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      </gitHub>
    </fluidoSkin>
  </custom>
    
  <bannerLeft>
    <name>${project.name}</name>
    <href>${project.url}</href>
  </bannerLeft>
  
  <bannerRight>
    <name>${project.description}</name>
  </bannerRight>

  <body>

	<links>
      <item name="Summary" href="" />
    </links>
      
    <menu name="Main">
      <item name="Home" href="https://github.com/barchart/barchart-udt/wiki"/>
      <item name="Build System" href="https://github.com/barchart/barchart-udt/wiki/Build-System"/>
    </menu>
    
    <menu name="Tutorials">
      <item name="Demo" href="https://github.com/barchart/barchart-udt/tree/master/barchart-udt-demo"/>
    </menu>
        
    <menu ref="reports"/>
        
  </body>

</project>
