<project default="javadoc">

	<target name="javadoc" description="make javadoc for google code web site">
		<javadoc access="protected"
		         author="true"
		         classpath="target/classes"
		         destdir="doc"
		         nodeprecated="false"
		         nodeprecatedlist="false"
		         noindex="false"
		         nonavbar="false"
		         notree="false"
		         source="1.6"
		         sourcepath="src/main/java"
		         splitindex="true"
		         use="true"
		         version="true">
			<link href="http://java.sun.com/javase/6/docs/api/" />
		</javadoc>
	</target>

</project>
